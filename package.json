  "repository": "dvargas92495/crowdinvestin.me",
  "scripts": {
    "format": "prettier --write \"**/*.tsx\"",
    "lint": "eslint . --ext .ts,.tsx",
    "api": "fuego api",
    "build": "fuego build",
    "compile": "fuego compile",
    "deploy": "fuego deploy",
    "fe": "fuego fe",
    "migrate": "prisma migrate deploy",
    "publish": "fuego publish",
    "precompile": "prisma generate",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "start": "concurrently \"npm:api\" \"npm:fe\""
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/node": "^16.11.11",
    "@types/pdfkit": "^0.12.3",
