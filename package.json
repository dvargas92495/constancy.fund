    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.4.0",
    "fuegojs": "^0.19.3",
    "prettier": "^2.5.1",
    "prisma": "^3.6.0",
    "ts-node": "^10.4.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@dvargas92495/api": "^0.5.1",
    "@dvargas92495/ui": "^0.45.2",
