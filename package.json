  "repository": "dvargas92495/constancy.fund",
  "scripts": {
    "format": "prettier --write \"**/*.tsx\"",
    "lint": "eslint . --ext .ts,.tsx",
    "api": "fuego api",
    "build": "fuego build --remix",
    "compile": "fuego compile",
    "deploy": "fuego deploy --remix",
    "dev": "fuego dev --remix",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "publish": "fuego publish",
    "precompile": "prisma generate",
    "start": "concurrently \"npm:api\" \"npm:dev\"",
    "postinstall": "fuego postinstall \"@mui/material\" \"pdfjs-dist\" \"@babel/runtime\"",
    "staging": "mysqlsh --host vargas-arts.c2sjnb5f4d57.us-east-1.rds.amazonaws.com --port 5432 --database staging_constancy_fund --user staging_constancy_fund",
    "test": "jest",
    "cypress": "cross-env CYPRESS_VERIFY_TIMEOUT=100000 cypress open",
    "cypress:staging": "cross-env CYPRESS_VERIFY_TIMEOUT=100000 ORIGIN=https://staging.constancy.fund cypress open"
  },
