{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2019",
    "allowJs": false,
    "lib": [
      "es2019",
      "dom",
      "dom.iterable"
    ],
    "baseUrl": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "~/*": [
        "./app/*"
      ]
    }
  },
  "include": [
    "app",
    "functions",
    "db",
    "remix.env.d.ts",
    "fonts.d.ts"
  ]
}