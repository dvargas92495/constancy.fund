datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contract {
  uuid String @id @default(uuid())
  type Int
  status Int
  userId String
  contractDetails ContractDetail[]
}

model ContractDetail {
  uuid String @id @default(uuid())
  label String
  value String
  contractUuid String
  contract Contract @relation(fields: [contractUuid], references: [uuid])
}
